function shuffel_deck(deck)
    size = 41
    --Algo shuffle
    while size > 0 do
        random_num = flr(rnd(41)) + 1
        temp = deck[size]
        deck[size] = deck[random_num]
        deck[random_num] = temp
        size -= 1
    end
    return deck
end

